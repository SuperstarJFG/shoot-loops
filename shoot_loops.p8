--blink
function blink_init()
	blink=0	
end
 
function blink_update()
 --counts up, resets 32nd frame
 blink = (blink + 1) % 32
end
 
function blink_draw()
	pal()
	if state==2 then
		--p1 heal
		if b.heal then
			pal(8,7)
		end
		--p1 low hp
		if blink<4 and
		p.hp<=p.max_hp/2 and p.hp!=0 then
		  pal(8,2)
		end
		--p2 heal
		if b2.heal then
			pal(12,7)
		end
		--p2 low hp
		if blink<4 and
		p2.hp<=p2.max_hp/2 and p2.hp!=0 then
		  pal(12,1)
		end
	end
end
