--sfx
function sfx_update()
	--shoot
	if (b.fire or b2.fire) then
		sfx(2)
	end
	--heal	
	if b.heal or b2.heal then
		sfx(0)
	end
	--hurt
	if (b.hurt or b2.hurt) then
		sfx(4)
	end
	--low hp
	if (p.hp<=p.max_hp/2 or 
				p2.hp<=p2.max_hp/2) and
				state==2 and blink==0 then
		sfx(3)
	end
end
