function intro_init()
	--initial arrow y
	intro1y=64
	intro2y=56
end

function intro_draw()
	--bg
	for i=0,256,2 do
		line(0,i-128,128,i,1)
		line(-1,i-128,127,i,2)
	end
	--draw arrrows
	spr(spr1+4,32,intro1y)
	spr(spr2+4,88,intro2y,1,1,false,true)
	--move arrows
	intro1y+=b.spd
	intro2y-=b2.spd
	--loop arrows
	if (intro1y>127) intro1y=-8
	if (intro2y<-7) intro2y=128
end
