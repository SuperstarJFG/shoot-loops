--collision
function collide(
	x1,y1,
	w1,h1,
	x2,y2,
	w2,h2)

	local xs=w1/2+w2/2
	local ys=h1/2+h2/2
	local xd=abs(x1+w1/2-(x2+w2/2))
	local yd=abs(y1+h1/2-(y2+h2/2))

	if xd<xs and yd<ys then
		return true
	end	
end
